<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vb" alignment="CENTER" onMousePressed="#move" prefHeight="641.0" prefWidth="165.0" spacing="7.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PrimaryController">
   <children>
      <Button fx:id="LoadVNC" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#loadVNC" opacity="0.6" text="加载远程桌面" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font></Button>
      <Button fx:id="ShutdownVNC" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#shutdownVNC" opacity="0.6" text="停止远程桌面" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="SystemMonitor" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#openSystemMonitor" opacity="0.6" text="任务管理器" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="ShowDesktop" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#showDesktop" opacity="0.6" text="显示桌面" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="DesktopsRolling" focusTraversable="false" minHeight="30.0" minWidth="145.0" opacity="0.6" text="切换桌面" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="InputmethodSwitch" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#inputmethodSwitch" opacity="0.6" text="关闭输入法" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="CtrlLock" focusTraversable="false" minHeight="30.0" minWidth="145.0" onMouseClicked="#ctrl" opacity="0.6" text="CTRL锁定" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="ShiftLock" focusTraversable="false" minHeight="30.0" minWidth="145.0" onMouseClicked="#shift" opacity="0.6" text="SHITF锁定" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="AltLock" focusTraversable="false" minHeight="30.0" minWidth="145.0" onMouseClicked="#alt" opacity="0.6" text="ALT锁定" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="WinLock" focusTraversable="false" minHeight="30.0" minWidth="145.0" onMouseClicked="#win" opacity="0.6" text="WIN锁定" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="AHK" focusTraversable="false" minHeight="30.0" minWidth="145.0" onMouseClicked="#ahks" opacity="0.6" text="自定义按键" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="Keyboard" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#keyboard" opacity="0.6" text="屏幕键盘" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="About" focusTraversable="false" minHeight="30.0" minWidth="145.0" onAction="#switchToSecondary" opacity="0.6" text="关于" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Label fx:id="info" alignment="CENTER" prefHeight="24.0" prefWidth="145.0" text="Java Remote Assistant">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
